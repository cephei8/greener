{
  "listeners": {
    "*:5096": {
      "pass": "routes"
    }
  },
  "applications": {
    "greener": {
      "type": "python 3",
      "working_directory": "/app",
      "path": "/app",
      "module": "app.app",
      "callable": "app"
    }
  },
  "routes": [
    {
      "match": {"uri": "/api/v1/*"},
      "action": {"pass": "applications/greener"}
    },
    {
      "match": {"uri": "/*"},
      "action": {"share": "/app/blazor$uri"}
    }
  ]
}
